@model ITest.Areas.Admin.Models.CreateQuestionViewModel
@{
    ViewData["Title"] = "EditQuestionPartial";
}

<div class="question-container accordion">
    <div>
        <h2 style="display: inline-block" class="question-count">
            Question 1
        </h2>
    </div>

    <div class="form-group question">
        <div>Description</div>
        <textarea asp-for="Content" class="form-control question summernote" rows="5" id="question-content"></textarea>
       @foreach (var answer in Model.Answers)
       {
           @Html.Partial("EditAnswerPartial", answer);
       }
    </div>
</div>
<div style="clear:both"></div>


